{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "sourceMap": true,
      "outDir": "./dist",
      "rootDir": "./",
      "strict": true,
      "lib": ["esnext"],
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*", "constants/**/*", "models/**/*"],
    "exclude": ["node_modules", "dist"]
  }